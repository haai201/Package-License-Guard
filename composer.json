{
  "name": "susoft/license-guard",
  "description": "License guard middleware for Laravel apps â€“ calls remote license server and blocks invalid installs.",
  "type": "library",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Susoft\\LicenseGuard\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Susoft\\LicenseGuard\\LicenseGuardServiceProvider"
      ]
